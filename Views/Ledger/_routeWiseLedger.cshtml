@model IEnumerable<DATAMODEL.VENDOR_ROUTE_ENTRY>
<div class="row">
    <div class="col-lg-12 table-responsive">
        <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th colspan="6">
                        <h3 class="headinglist" style="text-align:center"><strong>  ایشیا پولٹری پروٹین </strong></h3>
                    </th>
                </tr>
                <tr>
                    <th>Date</th>
                    <th>Route</th>
                    <th>Driver Name</th>
                    <th>Credit</th>
                    <th>Debit</th>
                    <th>Weight</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>@item.ROUTE_ENTREES.ENTRY_DATE</td>
                        <td>@item.VENDOR_BRANCH.ROUTE.NAME</td>
                        <td>@item.ROUTE_ENTREES.DRIVER.DR_NAME</td>
                        <td>@{ var credit = @item.V_WEIGHT * @item.RATE;}@credit</td>
                        <td>@item.VENDOR_BRANCH.PYMENTs.Where(p => p.TRANS_DATE == @item.ROUTE_ENTREES.ENTRY_DATE).Sum(pmt => pmt.ADVANCE_AMOUNT)</td>
                        <td>@item.V_WEIGHT</td>
                    </tr>

                }
            </tbody>
        </table>
    </div>

</div>


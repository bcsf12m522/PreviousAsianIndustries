@model DATAMODEL.BANK_RECORDS
<br />
<table class="table table-bordered table-hover table-striped ">
    <tbody>
        <tr><td> @Model.VENDOR_BRANCH.ROUTE.NAME</td><td>@Model.VENDOR_BRANCH.VENDOR_BRANCH_ID  @Model.VENDOR_BRANCH.VBNAME @Model.VENDOR_BRANCH.ADDRESS - کھاتہ نمبر </td></tr>
        <tr><td>New Amount Added </td><td> Rs:  @Model.AMOUNT</td></tr>
        <tr><td> Cheque # </td><td> @Model.CHEQUE_NO</td></tr>
        <tr><td> Bank Name</td><td> @Model.BANKS_NAME.BANK_NAME</td></tr>
        <tr><td> Transaction Date</td><td> @Model.CASH_DATE</td></tr>
        <tr>
            <td>Total Credit</td>
            <td> @ViewBag.totalCrDbt.Totalcredit</td>
        </tr>
        <tr>
            <td>Total Debit</td>
            <td> @ViewBag.totalCrDbt.TotalDebit</td>
        </tr>
    </tbody>
</table>
<br />





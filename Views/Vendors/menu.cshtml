@{
    ViewBag.Title = "RouteMenu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Vendors</h2><hr />
<div class="row">
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-users fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>All Vendors</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("index", "Vendors", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">All Routes</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-edit fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Vendor contract</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("AddnewVendor", "Vendors", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Add new Vendor</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-credit-card fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Vendor wise Rate change</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("ChangeVendorRate", "EditList", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Change Vendor rate</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-credit-card fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div> &nbsp;&nbsp;&nbsp;&nbsp;Route wise rate change</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("ChangeRouteVendorRate", "Editlist", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Route Wise Rate change</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>


</div>
<div class="row">
    

   
</div>


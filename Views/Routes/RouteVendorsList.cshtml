@model IEnumerable<DATAMODEL.CUSTOM_MODEL.VENDORCM>
@{
    ViewBag.Title = "RouteVendorsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RouteVendorsList</h2>
<div class="row">
    <div class="col-lg-12 table-responsive">
        <table class="table table-striped table-bordered table-hover" id="dataTables-1">
            @{int rowNo = 0;}
            <thead>
                <tr>
                    
                    <th>
                        Phone
                    </th>
                    <th>
                        Address
                    </th>

                    <th>
                        Name
                    </th>
                    <th>
                        Sr No.
                    </th>



                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                      
                        <td>
                            @item.PHONE_NO
                        </td>
                        <td>
                            @item.ADDRESS
                        </td>
                        <td>
                            @item.V_NAME
                        </td>
                        <td>
                            @{rowNo++;}@rowNo
                        </td>

                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>



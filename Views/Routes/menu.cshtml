@{
    ViewBag.Title = "RouteMenu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Routes</h2>
<div class="row">
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-truck fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>All Routes</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("index", "Routes", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">All Routes</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-edit fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Route Entries</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("Index","EditList", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Route Entries</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-check fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Verified Entries</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("IndexOfOKEntries", "EditList", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">All Verified Entries</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-credit-card fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div> &nbsp;&nbsp;&nbsp;&nbsp;Route wise rate change</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("ChangeRouteVendorRate", "Editlist", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Route Wise Rate change</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>


</div>
<div class="row">
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-bank fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Cash Heads</div>


                    </div>
                </div>
            </div>
            <a href="@Url.Action("index","HeadCash", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Add new Heads</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
    
    <div class="col-lg-3 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class=" fa fa-credit-card fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge"></div>
                        <div>Vendor wise Rate change</div>
                    </div>
                </div>
            </div>
            <a href="@Url.Action("ChangeVendorRate", "EditList", null)" target="_blank">
                <div class="panel-footer">
                    <span class="pull-left">Change Vendor rate</span>
                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                    <div class="clearfix"></div>
                </div>
            </a>
        </div>
    </div>
</div>


@model DATAMODEL.BANK_RECORDS
@{
    ViewBag.Title = "CancleCheque";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cancle Cheque</h2>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <table class="table table-bordered table-hover table-striped ">
            <thead>
                <tr><th>Name</th><th>@Model.VENDOR_BRANCH.VBNAME @Model.VENDOR_BRANCH.ADDRESS</th></tr>
            </thead>
            <tbody>
                <tr><td>cheque No</td><td>@Model.CHEQUE_NO</td></tr>
                <tr><td>Amount </td><td>@Model.AMOUNT</td></tr>
                <tr><td>Bank  </td><td>@Model.BANKS_NAME.BANK_NAME</td></tr>
                <tr><td>Cash Date </td><td>@Model.CASH_DATE.Value.ToShortDateString()</td></tr>
                <tr><td colspan="2"><a href="@Url.Action("ConfirmCancleCheque", new { BankID=@Model.BANK_ID })" class="btn btn-success">Confirm</a></td></tr>
            </tbody>
        </table>
    </div>
</div>
